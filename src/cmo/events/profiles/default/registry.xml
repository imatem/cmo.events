<?xml version="1.0"?>
<registry>
    <records interface="cmo.events.browser.controlpanel.ICMOControlPanel"
         prefix="cmo" />
    <records prefix="plone.resources/tablecmo"
            interface='Products.CMFPlone.interfaces.IResourceRegistry'>
        <value key="js">++plone++cmo/tablecmo.js</value>
    </records>

    <records prefix="plone.bundles/datatables"
           interface='Products.CMFPlone.interfaces.IBundleRegistry'>
    <value key="enabled">True</value>
    <value key="jscompilation">++plone++cmo/datatables/js/datatables.min.js</value>
    <value key="csscompilation">++plone++cmo/datatables/css/datatables.min.css</value>
    <value key="compile">False</value>
    <value key="depends">plone</value>
  </records>

</registry>


